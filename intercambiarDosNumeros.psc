Algoritmo intercambiarDosNumeros
	
	Definir Valor1, Valor2 Como Entero;
	
	Escribir "Favor ingresar el primer número.";
	Leer Valor1;
	Escribir "Favor ingresar el segundo número.";
	Leer Valor2;
	
	Escribir "El Valor 1 es: ", Valor1, ", y el valor 2 es: ", Valor2;
	
	Valor1 <- Valor2;
	Valor2 <- Valor1;
	Escribir "Intercambiando el valor 1 es: ", Valor1, ", y el valor 2 es: ", Valor2;
	
FinAlgoritmo